{
  "name": "nativescript-tslib",
  "version": "1.8.1-beta.2",
  "description": "Add support for tslib helpers in NativeScript projects",
  "main": "dist/index",
  "nativescript": {
    "platforms": {
      "ios": "3.3.0",
      "android": "3.3.0"
    },
    "plugin": {
      "nan": "true",
      "pan": "true",
      "core3": "true",
      "category": "Utilities"
    }
  },
  "scripts": {
    "rm:dist": "rimraf dist",
    "build": "tsc",
    "prepublish": "npm-run-all rm:dist build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/m-abs/nativescript-tslib.git"
  },
  "author": "Morten Sj√∏gren <m_abs@mabs.dk>",
  "license": "git@github.com:m-abs/nativescript-tslib.git",
  "bugs": {
    "url": "https://github.com/m-abs/nativescript-tslib/issues"
  },
  "homepage": "https://github.com/m-abs/nativescript-tslib#readme",
  "dependencies": {
    "tslib": "~1.8.1"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.2",
    "rimraf": "^2.6.2",
    "tns-core-modules": "^3.3.0",
    "typescript": "^2.3.4"
  }
}
